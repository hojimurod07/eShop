                {
                    <a asp-controller="home" asp-action="Filtered" asp-route-categoryId="@cat.Id" class="text-white bg-gray-800 hover:bg-gray-900 focus:outline-none focus:ring-4 focus:ring-gray-300 font-medium rounded-full text-sm px-5 py-2.5 me-2 mb-2 dark:bg-gray-800 dark:hover:bg-gray-700 dark:focus:ring-gray-700 dark:border-gray-700">@cat.Name</a>
                }
                
            </ul>

         <div class="filters-content">
                <div class="row grid">

                    @foreach(var prod in Model.Products)
                    {
                        <div class="col-sm-6 col-lg-4 all">
                            <div class="box">

                                <div>
                                    <div style="position: relative;">
                                        <a href="#">
                                            <img style="object-fit: cover; width: 100%; height: auto; max-height: 200px; border-bottom-left-radius: 50px;" src="@prod.ImageUrl" asp-append-version="true" alt="">
