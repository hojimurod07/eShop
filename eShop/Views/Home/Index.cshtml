                            <!-- Item 1 -->
                            @foreach (var c in Model.Products)
                            {
                                <div class="hidden duration-700 ease-in-out" data-carousel-item>
                                    <img style="object-fit:contain" src="@c.ImageUrl" class="  absolute block w-full -translate-x-1/2 -translate-y-1/2 top-1/2 left-1/2" alt="...">
                                </div>
                            }




                        </div>
                        <!-- Slider indicators -->
