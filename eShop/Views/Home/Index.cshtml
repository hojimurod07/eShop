                            <script>
                                var CardCount = document.getElementById("card-caunt-item");
                                var cartItems = JSON.parse(localStorage.getItem('cartItems')) || [];

                                // Function to update the modal table with cart items
                                function updateModalTable() {
                                    var modalTableBody = document.querySelector("#static-modal tbody");
                                    modalTableBody.innerHTML = ''; // Clear the table body

                                    cartItems.forEach(item => {
                                        var row = document.createElement('tr');
                                        row.classList.add('bg-white', 'border-b', 'dark:bg-gray-800', 'dark:border-gray-700', 'hover:bg-gray-50', 'dark:hover:bg-gray-600');

                                        // Product Image
                                        var imgCell = document.createElement('td');
                                        imgCell.classList.add('p-4');
                                        var img = document.createElement('img');
                                        img.setAttribute('src', `/docs/images/products/${item.prodId}.png`);
                                        img.setAttribute('alt', `${item.prodId}`);
                                        img.classList.add('w-16', 'md:w-32', 'max-w-full', 'max-h-full');
                                        imgCell.appendChild(img);
                                        row.appendChild(imgCell);

                                        // Product Name
                                        var nameCell = document.createElement('td');
                                        nameCell.classList.add('px-6', 'py-4', 'font-semibold', 'text-gray-900', 'dark:text-white');
                                        nameCell.textContent = item.prodId;
                                        row.appendChild(nameCell);

                                        // Quantity
                                        var qtyCell = document.createElement('td');
                                        qtyCell.classList.add('px-6', 'py-4');
                                        qtyCell.innerHTML = `<div class="flex items-center">
                                                                    <input type="number" class="bg-gray-50 w-14 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block px-2.5 py-1 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" value="${item.count}" required />
                                                                </div>`;
                                        row.appendChild(qtyCell);

                                        // Price (assuming a fixed price for simplicity)
                                        var priceCell = document.createElement('td');
                                        priceCell.classList.add('px-6', 'py-4', 'font-semibold', 'text-gray-900', 'dark:text-white');
                                        priceCell.textContent = '$599'; // You can customize this based on your data structure
                                        row.appendChild(priceCell);

                                        // Action (remove button)
                                        var actionCell = document.createElement('td');
                                        actionCell.classList.add('px-6', 'py-4');
                                        var removeButton = document.createElement('button');
                                        removeButton.textContent = 'Remove';
                                        removeButton.classList.add('font-medium', 'text-red-600', 'dark:text-red-500', 'hover:underline');
                                        removeButton.addEventListener('click', function () {
                                            removeItem(item.prodId);
                                        });
                                        actionCell.appendChild(removeButton);
                                        row.appendChild(actionCell);

                                        modalTableBody.appendChild(row);
                                    });

                                    // Update the total count
                                    var totalCount = cartItems.reduce((acc, item) => acc + item.count, 0);
                                    CardCount.textContent = totalCount;
                                }

                                // Call the function to initially populate the modal table
                                updateModalTable();

                                // Function to add an item to the cart
                                function Add(prodId) {
                                    var existingItemIndex = cartItems.findIndex(item => item.prodId === prodId);
                                    if (existingItemIndex !== -1) {
                                        cartItems[existingItemIndex].count++;
                                    } else {
                                        cartItems.push({ prodId: prodId, count: 1 });
                                    }

                                    localStorage.setItem('cartItems', JSON.stringify(cartItems));
                                    updateModalTable(); // Update the modal table with the latest cart items
                                    
                                }

                                // Function to remove an item from the cart and update the modal
                                function removeItem(prodId) {
                                    cartItems = cartItems.filter(item => item.prodId !== prodId); // Remove the item from the cart
                                    localStorage.setItem('cartItems', JSON.stringify(cartItems)); // Update localStorage
                                    updateModalTable(); // Update the modal table with the latest cart items
                                    alert('Product ' + prodId + ' removed from cart.');
                                }
                            </script>


